#include <stdio.h>
#include <string.h>
int find_indexF(char [],char []);
int main()
{
  char str1[100];
  printf("Enter the string:1: ");
  fgets(str1,sizeof(str1),stdin);
  str1[strcspn(str1,"\n")]='\0';
  
  char str2[100];
  printf("Enter the string-2: ");
  fgets(str2,sizeof(str2),stdin);
  str2[strcspn(str2,"\n")]='\0';
  
  find_indexF(str1,str2);
  

}
