#include <stdio.h>
int main()
{
int i,j,row,col,count=0;
int identity = 0;
printf("Enter the no.of rows: ");
scanf("%d",&row);
printf("Enter the no.of columns: ");
scanf("%d",&col);
int a[row][col];
printf("Enter the elements: ");
for (i=0;i<row;i++)
{
  for (j=0;j<col;j++)
  {
    scanf("%d",&a[i][j]);
  }
}

printf("Matrix: \n")
for (i=0;i<row;i++)
{
  for (j=0;j<col;j++)
  {
    printf("%d ",&a[i][j]);
  }
}

for (i=0;i<row;i++)
{
  identity = 0;
  for (j=0;j<col;j++)
  {
    if ( ((i==j)&&(a[i][j]==1)) || ((i>j || i<j) && a[i][j]==0))
    {
      identity = 1;
    }
  }
}

if (identity == 1)
{
  printf("Identity matrix...");
}
else
{
  printf("Not an identity matrix");
}


}
