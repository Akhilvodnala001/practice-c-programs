#include <stdio.h>
#include <stdlib.h>
struct node
{
  int data;
  struct node *left;
  struct node *right;
};
struct node *create(void)
{
  int x;
  printf("Enter the data(-1 for no node): ");
  scanf("%d",&x);
  if (x == -1)
  {
    return 0;
  }
}
int main()
{
  struct node *root;
  root = create();
  

}
